<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <size android:width="@dimen/back_round_width" android:height="@dimen/back_round_height"/>
            <solid android:color="@color/back_selected"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <size android:width="@dimen/back_round_width" android:height="@dimen/back_round_height"/>
            <solid android:color="@color/back_normal"/>
        </shape>
    </item>
</selector>